{
  "name": "vue-mobx-prime-ts",
  "version": "2.4.0",
  "private": true,
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "browser-unhandled-rejection": "^1.0.2",
    "echarts-jsx": "^1.2.1",
    "idb-keyval": "^6.2.2",
    "koajax": "^3.1.2",
    "mobx": "^6.15.0",
    "mobx-downloader": "^0.3.1",
    "mobx-restful": "^2.1.3",
    "mobx-vue-lite": "^0.4.3",
    "primevue": "^4.4.0",
    "vue": "^3.5.22",
    "vue-facing-decorator": "^4.0.1",
    "vue-router": "^4.5.1",
    "web-streams-polyfill": "^4.2.0",
    "web-utility": "^4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@octokit/openapi-types": "^26.0.0",
    "@parcel/packager-raw-url": "~2.16.0",
    "@parcel/transformer-vue": "~2.16.0",
    "@parcel/transformer-webmanifest": "~2.16.0",
    "@types/node": "^22.18.8",
    "@vue/tsconfig": "^0.8.1",
    "buffer": "^6.0.3",
    "husky": "^9.1.7",
    "less": "^4.4.1",
    "lint-staged": "^16.2.3",
    "parcel": "~2.16.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0",
    "workbox-cli": "^7.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "vue-demi"
    ]
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{html,md,css,less,json,yml,js,ts,mts,vue}": "prettier --write"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead, IE 11",
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf .parcel-cache/ dist/",
    "start": "npm run clean  &&  parcel src/index.html",
    "pack": "npm run clean  &&  vue-tsc --noEmit  &&  parcel build src/index.html --public-url .",
    "build": "npm run pack  &&  workbox generateSW",
    "test": "lint-staged"
  }
}
